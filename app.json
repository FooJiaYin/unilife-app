{
  "expo": {
    "name": "UniLife",
    "slug": "unilife",
    "scheme": "unilife",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.0.4",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "cc.UniLife.UniLife",
      "buildNumber": "2.0.4",
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "我們需要取用您的相片，讓您在撰寫貼文時可以選取照片"
      }
    },
    "android": {
      "package": "cc.unilife.unilife",
      "versionCode": 21,
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/android-notification.png",
      "color": "#00aebb"
    },
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/Unilife-App/unilife-app",
    "description": ""
  }
}
